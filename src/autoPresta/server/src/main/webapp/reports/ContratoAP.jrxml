					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[d)]]></text>
			</staticText>
			<staticText>
				<reportElement x="27" y="0" width="23" height="17" uuid="a8859912-03d2-41b6-ae45-5327e84f02e2"/>
				<textElement textAlignment="Center" verticalAlignment="Top" markup="styled">
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[c)]]></text>
